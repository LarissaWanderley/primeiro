@model IList<Inicial.Dominio.Usuario>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome </th>
            <th>E-Mail</th>
            <th>@Html.ActionLink("Cadastrar", "Form", "Usuario")</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
            {
            <tr>
                <td>@usuario.Id</td>
                <td>@Html.ActionLink(usuario.Nome, "Visualiza", new { id = usuario.Id })</td>
                <td>@usuario.Email</td>
                <td>@Html.ActionLink("Excluir", "Excluir", new { id = usuario.Id })</td>
            </tr>
        }
    </tbody>
</table>

